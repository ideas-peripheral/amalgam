{
  "scripts": {
    "lint": "eslint src/*.ts --fix && tsc --noEmit",
    "watch": "parcel watch src/index.html --public-url ./",
    "build": "rm -fr dist/ && parcel build src/index.html --public-url ./",
    "predeploy": "npm run lint",
    "deploy": "npm run build"
  },
  "dependencies": {
    "vue": "^3.0.0-alpha.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3",
    "electron": "^9.4.0"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "private": true
}
